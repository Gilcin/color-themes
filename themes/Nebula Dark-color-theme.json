				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#6A88FF" // Operadores mais destacados
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag", "meta.tag.sgml"],
			"settings": {
				"foreground": "#FF4E6A", // Tags mais vibrantes
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#50FA7B", // Funções mais vivas
				"fontStyle": "bold" // Destaque adicional para funções
			}
		},
		{
			"name": "Block Level Variables",
			"scope": ["meta.block variable.other"],
			"settings": {
				"foreground": "#C8D1FF"
			}
		},
		{
			"name": "Other Variable, String Link",
