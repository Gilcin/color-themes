				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#8BE9FD"
			}
		},
		{
			"name": "Entity Types",
			"scope": ["support.type"],
			"settings": {
				"foreground": "#8BE9FD"
			}
		},
		{
			"name": "CSS Class and Support",
