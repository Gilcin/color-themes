				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#FFB86C" // Números e constantes mais vibrantes
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
