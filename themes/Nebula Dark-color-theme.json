	"tokenColors": [
		{
			"name": "Comments",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#5D77A0", // Comentários mais claros para melhor leitura
				"fontStyle": "italic"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable", "string constant.other.placeholder"],
			"settings": {
				"foreground": "#BD93F9" // Variáveis mais claras
			}
		},
		{
			"name": "Colors",
			"scope": ["constant.other.color"],
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#FF3C5A"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#A687FF", // Palavras-chave mais vivas
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.operator",
				"constant.other.color",
				"punctuation",
